    "magento/module-backend": "100.0.*|100.1.*|100.2.*|101.0.0",
    "magento/module-directory": "100.0.*|100.1.*|100.2.*|100.3.*",
    "magento/framework": "100.0.*|100.1.*|101.0.*|102.0.*"
  },
  "require-dev": {
    "magento/zendframework1": "1.12.*|1.13.*|1.14.*",
